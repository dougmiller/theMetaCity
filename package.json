{
  "name": "themetacityflask",
  "version": "0.0.1",
  "description": "Build for the MetaCity main site",
  "private": true,
  "author": "Douglas Miller",
  "scripts": {
    "lint": "jshint theMetaCityFlask/static/javascript/*.js",
    "ts": "ts typescript/*.ts",
    "css": "npm run css:fp & npm run css:main",
    "css:fp": "npm run css:fp:dev",
    "css:fp:dev": "node FPPostCSS-dev.js",
    "css:fp:prod": "node FPPostCSS-prod.js",
    "css:main": "npm run css:main:dev",
    "css:main:dev": "node CSSPostCSS-dev.js",
    "css:main:prod": "node CSSPostCSS-prod.js",
    "css:watch": "watch 'npm run css' ./pcss",
    "deploy": "npm run deploy:local",
    "deploy:local": "node Deploy-local.js",
    "deploy:prod": "node Deploy-prod.js"
  },
  "dependencies": {
    "autoprefixer": "latest",
    "caniuse-lite": "latest",
    "cssnano": "latest",
    "electron-to-chromium": "latest",
    "fs-extra": "latest",
    "chownr": "latest",
    "jshint": "latest",
    "node-releases": "latest",
    "npm": "latest",
    "postcss": "latest",
    "postcss-css-variables": "latest",
    "postcss-custom-media": "latest",
    "postcss-discard-comments": "latest",
    "postcss-import": "latest",
    "postcss-import-part": "latest",
    "postcss-media-variables": "latest",
    "postcss-nested": "latest",
    "postcss-partial-import": "latest",
    "postcss-reporter": "latest",
    "typescript": "latest",
    "watch": "latest"
  },
  "devDependencies": {
    "postcss-map": "latest"
  }
}
